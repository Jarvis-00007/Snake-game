import pygame
import random
class orb:
    def __init__(self,x,y,h,w):
        self.rect=pygame.Rect(x,y,h,w)
        image=pygame.image.load("C.png")
        self.texture=pygame.transform.scale(image,(h,w))
    def update(self):
        pass
    def draw(self,window):
        window.blit(self.texture,(self.rect.x,self.rect.y))
